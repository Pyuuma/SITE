@model IEnumerable<Web.Models.IdentityRecord>

@{
    ViewBag.Title = "IdentityRecords";
}
<style>
    .nav-pills > li > a {
        color: white;
    }
    h2 {
        font-family: 'Microsoft YaHei';
        color: #577492;
        text-align: center;
    }

    .table > tbody > tr > th {
        text-align: center;
    }

    .table > tbody > tr > td {
        text-align: center;
    }
    dl{
        text-align:center;
    }
    dd{
        color:#3C5B45;
        font-family:Arial;
        font-size:larger;
    }
    dt {
        color: #608D4E;
        font-family: Arial;
        font-size: larger;
    }

    body {
        background: url(/img/HeaderGreyDenim.jpg);
        background-repeat: repeat;
    }

    .nav-list > li > a {
        padding: 5px 8px;
    }

</style>

<div class="col-md-12" style="display:block;height:300px;padding-left:0;padding-right:0;background-image:url(../../img/qiusuo1.jpg);background-size:cover;">

</div>
<div class="col-md-12 col-xs-12">
    <div class="col-md-2 col-xs-2">
        @Html.Partial("_AdminMenuPartial")
    </div>
    <div class="col-md-10 col-xs-10">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.User.DisplayName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>

                    @Html.DisplayNameFor(model => model.InUIBE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsStudent)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Time)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)

            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.User.DisplayName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.InUIBE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IsStudent)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Time)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>

                    <td>
                        @Html.ActionLink("Details", "IdentityRecordDetails", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>

    </div>
</div>
    
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdentityRecord.IdNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Project.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamRecord.RemarkContent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DisplayName)
        </td>        
        <td>
            @Html.DisplayNameFor(model => model.Profile.Email)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.Profile.InformationPrivacy)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.Profile.Other)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.Profile.Phone)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.Profile.Searchable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Identitied)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsDisabled)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailConfirmed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecurityStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumberConfirmed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LockoutEndDateUtc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LockoutEnabled)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessFailedCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.ActionLink("Edit", "UserEdit", new { id=item.Id })
        </td>
    </tr>
}

</table>

@model IEnumerable<Web.Models.Company>

@{
    ViewBag.Title = "CompanyIdentityRecords";
}

<section class="sec2">
    <div class="cover">
    </div>
    <div class="row animated pulse">
        <b class="secheader">管理员 | 公司列表 </b>
        <p class="seccontent">
            京贸创客创业平台是对外经济贸易大学国际经济贸易学院与北京其他兄弟院校共建的创业平台。在这里你能找到自己的创业伙伴，获得导师指导，注册并成立自己的创业公司。
        </p>
    </div>
</section>
<section style="display: table;width: 100%;height: auto; padding: 100px 0 100px; color:black;position: relative;">
    <div class="row" style="padding:0 5%;">
        <div class="col-xs-2">
            @Html.Partial("_AdminMenuPartial")
        </div>
        <div class="col-xs-10 board">
            <h4>公司列表</h4>
            <hr />
            <table class="table table-striped">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Admin.DisplayName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Admin.Identitied)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Admin.UserName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Admin.PhoneNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SourcesOfFunds)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Corporation)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.RegisterSite)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CodeOfBusinessLicense)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.MembersCount)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AmountOfMoney)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.RecentTurnover)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CompanyValuation)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FinancingAmount)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SharesSold)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FinancingTime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Investment)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Note)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Time)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Status)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
            {
                    <tr>
                        <td>
                            @Html.DisplayNameFor(model => model.Admin.DisplayName)
                        </td>
                        <td>
                            @Html.DisplayNameFor(model => model.Admin.Identitied)
                        </td>
                        <td>
                            @Html.DisplayNameFor(model => model.Admin.UserName)
                        </td>
                        <td>
                            @Html.DisplayNameFor(model => model.Admin.PhoneNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SourcesOfFunds)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Corporation)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RegisterSite)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CodeOfBusinessLicense)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MembersCount)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AmountOfMoney)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RecentTurnover)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CompanyValuation)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FinancingAmount)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SharesSold)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FinancingTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Investment)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Note)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Time)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Status)
                        </td>
                        <td>
                            @Html.ActionLink("Details", "IdentityRecordDetails", new { id = item.Id })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</section>

